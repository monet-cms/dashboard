{
    "name": "monet/dashboard",
    "description": "The dashboard module for the Monet framework.",
    "keywords": [
        "monet-cms",
        "laravel",
        "dashboard"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.3",
        "illuminate/support": "^9.3",
        "monet/auth": "dev-main",
        "monet/support": "dev-main",
        "spatie/laravel-dashboard": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Monet\\Framework\\Dashboard\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-dashboard"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
